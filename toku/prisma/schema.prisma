// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
 url = env("DATABASE_URL")
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  password String
}

model Heroes {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  Era String 
  Image String 
  Info String 
}